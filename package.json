{
  "name": "feathers-react",
  "version": "0.5.1",
  "description": "A feathers-aware component",
  "author": "silvestreh",
  "license": "MIT",
  "repository": "silvestreh/feathers-react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "cross-env CI=1 react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom --coverage --watch",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "release:patch": "npm test && npm version patch && git push origin --tags && npm publish",
    "release:minor": "npm test && npm version minor && git push origin --tags && npm publish",
    "release:major": "npm test && npm version major && git push origin --tags && npm publish",
    "lint": "semistandard --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/test.js",
      "!src/setupTests.js",
      "!src/test-client.js",
      "!src/index.js",
      "!src/languages.js"
    ]
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": {
      "es2017": true,
      "es6": true,
      "jest": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-function-bind": "^7.12.13",
    "@babel/preset-react": "^7.13.13",
    "@feathersjs/client": "^4.5.11",
    "@svgr/rollup": "^5.5.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.27.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-semistandard-react": "^4.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-semistandard-react": "^4.1.0",
    "eslint-plugin-standard": "^4.1.0",
    "faker": "^5.5.3",
    "lodash.times": "^4.3.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^4.0.3",
    "rollup": "^2.50.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-url": "^3.0.1",
    "semistandard": "^16.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "rc-pagination": "^3.1.6",
    "sift": "^13.5.4"
  }
}
